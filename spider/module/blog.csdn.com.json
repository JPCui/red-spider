{
	"blog.csdn.com": {
	    "db" : "blog_csdn_com",
		"url": "http://blog.csdn.net/dQCFKyQDXYm3F8rB0",
        "seedDiscoveries": [{
            "pattern": "http://blog.csdn.net/.*/article/list/(.*)",
            "type" : "10",
            "" : 0,
		}],
        // 列表页中，用于定位列表DOM
        "parentAttr" : {
            "parserPath" : "#article_list div.list_item.article_item",
            "field" : "",
            "parserType" : 20
        },
        // 属性列表
        "attrs" : [{
                "parserPath" : ".article_title .link_title a",
                "parserPathAttr" : "text",
                "field" : "title",
                "parserType" : 20
            },{
                "parserPath" : ".article_manage .link_view",
                "parserPathAttr" : "text",
                "field" : "view_num",
                "parserType" : 20,
                "nested" : {
                    "parserPath" : "\\((.*)\\)",
                    "parserType" : 30
                }
            },{
                "parserPath" : ".article_manage .link_comments",
                "parserPathAttr" : "text",
                "field" : "cmt_num",
                "parserType" : 20,
                "nested" : {
                    "parserPath" : "\\((.*)\\)",
                    "parserType" : 30
                }
            }
        ],
        // 是否是列表页
        "isList" : 1,
        // 是否跳过存储
        "skip" : 0
    }
}