{
  "type": "parser",
  "ruleName": "99lib-book",
  "urlPattern": "http://www.99lib.net/book/index.php(\\?page=(.*))?",
  "parentAttr": {
    "parserPath": "#right .list_box li",
    "field": "",
    "parserType": 20
  },
  "attrs": [
    {
      "field": "_tableName",
      "defaultValue": "99lib-book",
      "parserType": 0
    },
    {
      "field": "_source",
      "defaultValue": "99lib",
      "parserType": 0
    },
    {
    	"field" : "book_id",
    	"parserPath" : "h2 a",
        "parserPathAttr": "href",
    	"parserType" : 20,
    	"unique" : true,
    	"nested" : {
    		"parserType" : 30,
    		"parserPath" : "/book/(.*)/index.htm"
    	}
    },
    {
      "parserPath": "h4:eq(3) a",
      "parserPathAttr": "text",
      "field": "type",
      "parserType": 20
    },
    {
      "parserPath": "h4:eq(4) a",
      "parserPathAttr": "allText",
      "field": "tags",
      "parserType": 20,
      "hasMultiValue" : true
    },
    {
      "parserPath": "h2 a",
      "parserPathAttr": "text",
      "field": "name",
      "parserType": 20
    },
    {
      "parserPath": "h4:eq(2) a",
      "parserPathAttr": "text",
      "field": "author",
      "parserType": 20
    },
    {
      "parserPath": "div.intro",
      "parserPathAttr": "allText",
      "field": "summary",
      "parserType": 20
    }
  ],
  "isList": 1
}
