{
	// 图书列表
	"type" : "parser",
	"ruleName" : "99lib-book",
	"urlPattern" : "http://www.99lib.net/book/index.php?page=(.*)",
    // 列表页中，用于定位列表DOM，非必须
    "parentAttr" : {
        "parserPath" : "#right .list_box li",
        "field" : "",
        "parserType" : 20
    },
    // 属性列表
    "attrs" : [{
            "parserPath" : "a.type",
            "parserPathAttr" : "text",
            "field" : "type",
            "parserType" : 20,
            "nested" : {
                "parserPath" : "\\[(.*)\\]",
                "parserType" : 30
            }
        },{
            "parserPath" : "a.name",
            "parserPathAttr" : "text",
            "field" : "name",
            "parserType" : 20
        },{
            "parserPath" : "span",
            "parserPathAttr" : "text",
            "field" : "author",
            "parserType" : 20,
            "nested" : {
                "parserPath" : "\\((.*)\\)",
                "parserType" : 30
            }
        }
    ],
    // 是否是列表页
    "isList" : 1,
    // 是否跳过存储
    "skip" : 0
}