{
  // 章节标题列表
  "type": "parser",
  "ruleName": "99lib-book-sections",
  "urlPattern": "http://www.99lib.net/book/(.*)/(.*).htm",
  // 用于：1. 区别文档类型；2. 缩小范围
  "parentAttr": {
    "parserType": 20,
    "parserPath": "#content"
  },
  // 属性列表
  "attrs": [
    {
      "field": "_tableName",
      "defaultValue": "99lib-book-sections",
      "parserType": 0
    },
    {
      "parserPath": "#content div",
      "parserPathAttr": "allText",
      "field": "content",
      "parserType": 20,
      "isMulti" : 1,
      "filterRepeat" : 1
    }
  ],
  // 是否是列表页
  "isList": 0,
  // 是否跳过存储
  "skip": 0
}