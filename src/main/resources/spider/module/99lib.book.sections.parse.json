{
  "type": "parser",
  "ruleName": "99lib-book-sections",
  "urlPattern": "http://www.99lib.net/book/(.*)/(.*).htm",
  "parentAttr": {
    "parserType": 20,
    "parserPath": "#content"
  },
  "denoisingTypes" : [20],
  "attrs": [
    {
      "field": "_tableName",
      "defaultValue": "99lib-book-sections",
      "parserType": 0
    },
    {
      "field": "_source",
      "defaultValue": "99lib",
      "parserType": 0
    },
    {
      "field": "chaptor_id",
      "parserPath": "http://www.99lib.net/book/.*/(.*).htm",
      "parserType": 50,
      "unique": true
    },
    {
      "field": "book_id",
      "parserPath": "http://www.99lib.net/book/(.*)/.*.htm",
      "parserType": 50
    },
    {
      "parserPath": "#content div",
      "parserPathAttr": "allText",
      "field": "content",
      "parserType": 20,
      "hasMultiValue": true,
      "denoisingType" : 10
    }
  ],
  "isList": 0
}
