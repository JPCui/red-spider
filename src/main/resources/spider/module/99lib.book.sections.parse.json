{
  // 章节标题列表
  "type": "parser",
  "ruleName": "99lib-book-sections",
  "urlPattern": "http://www.99lib.net/book/(.*)/(.*).htm",
  // 用于：1. 区别文档类型；2. 缩小范围
  "parentAttr": {
    "parserType": 20,
    "parserPath": "#content"
  },
  // 属性列表
  "attrs": [
    {
      "field": "_tableName",
      "defaultValue": "99lib-book-sections",
      "parserType": 0
    },
    {
      "field": "section_id",
      "parserPath": "http://www.99lib.net/book/.*/(.*).htm",
      "parserType": 50,
      "uniqueFlag": true
    },
    {
      "field": "chaptor_id",
      "parserPath": "http://www.99lib.net/book/(.*)/.*.htm",
      "parserType": 50
    },
    {
      "parserPath": "#content div",
      "parserPathAttr": "allText",
      "field": "content",
      "parserType": 20,
      "hasMultiValue": true,
      "filterRepeat": 1
    }
  ],
  // 是否是列表页
  "isList": 0
}